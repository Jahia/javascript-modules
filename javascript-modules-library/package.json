{
  "name": "@jahia/javascript-modules-library",
  "version": "${project.version}",
  "type": "module",
  "main": "index.js",
  "types": "types/index.d.ts",
  "repository": "git@github.com:Jahia/javascript-modules.git",
  "license": "MIT",
  "scripts": {
    "types:copy": "mkdir -p dist/types && ncp target/types dist/types",
    "build:development": "run types:copy && tsc && node generate-index.cjs",
    "build:production": "run build:development && run minify",
    "build": "run build:production",
    "minify": "node minify.cjs",
    "doc": "typedoc",
    "clean": "rm -rf dist",
    "lint": "eslint --ext js,jsx,json,ts src/",
    "lint:fix": "eslint --ext js,jsx,json,ts --fix src/",
    "prepack": "echo 'Please run 'mvn package' to create the tgz so the Maven version can be injected in the package.json' && exit 1"
  },
  "bin": {
    "jahia-deploy": "bin/jahia-deploy.mjs"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "dotenv": "^16.4.7",
    "graphql": "^16.0.1",
    "graphql-tag": "^2.12.6",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "i18next": ">=23.0.0 <23.11.0",
    "react": ">=18.0.0 <18.3.0",
    "react-dom": ">=18.0.0 <18.3.0",
    "react-i18next": ">=14.0.0 <14.2.0",
    "styled-jsx": ">=5.0.0 <5.2.0"
  },
  "peerDependenciesMeta": {
    "i18next": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-i18next": {
      "optional": true
    },
    "styled-jsx": {
      "optional": true
    }
  },
  "resolutions": {
    "minimist": "^1.2.6",
    "set-getter": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@jahia/eslint-config": "^2.1.2",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "i18next": "^23.10.1",
    "ncp": "^2.0.0",
    "react": "^18.2.0",
    "react-i18next": "^14.1.0",
    "terser": "^5.36.0",
    "typedoc": "^0.25.13",
    "typescript": "^4.3.5"
  },
  "packageManager": "yarn@4.5.0"
}
