{
  "name": "$$MODULE_NAME$$",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/server/index.js",
  "files": [
    "dist/client/**/*",
    "dist/server/**/*",
    "settings",
    "static"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build && run package",
    "clean": "shx rm -rf dist/",
    "deploy": "jahia-deploy",
    "format": "prettier --write --list-different .",
    "lint": "eslint .",
    "package": "shx mkdir -p dist && yarn pack --out dist/package.tgz",
    "watch": "vite build --watch",
    "watch:callback": "run build && run deploy"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.26.2",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.20.0",
    "@jahia/javascript-modules-library": "^0.5.1",
    "@jahia/vite-plugin": "^0.5.1",
    "@types/react": "^19.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-cypress": "^4.1.0",
    "globals": "^15.15.0",
    "i18next": "^23.10.1",
    "prettier": "^3.5.1",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "react-i18next": "^15.4.0",
    "shx": "^0.3.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0"
  },
  "packageManager": "yarn@4.3.1",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  },
  "jahia": {
    "snapshot": true,
    "module-dependencies": "default",
    "module-type": "templatesSet",
    "module-type-comment": "Use templatesSet in the module type to declare a template set",
    "server": "dist/server/index.js",
    "static-resources": "/icons,/images,/javascript,/locales"
  },
  "dependencies": {
    "modern-normalize": "^3.0.1"
  }
}
